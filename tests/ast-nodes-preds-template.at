m4_define([sexpr],
  [(make-nix-data-node
    (nix-path-node?
     (list->nix-attributepath-node `("a" "b"))))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_false],[])

m4_define([sexpr],
  [(make-nix-data-node
    (nix-attributepath-node?
     (list->nix-attributepath-node `("a" "b"))))])
