AT_SETUP([setup_message(AST node predicates)])

m4_define([sexpr],
  [(make-nix-data-node
    (nix-embedded-node?
     (make-nix-embedded-node "let a = 4; b = a + 5; in b")))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_true],[])

m4_define([sexpr],
  [(make-nix-data-node (nix-embedded-node? 12345))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_false],[])

m4_define([sexpr],
  [(make-nix-data-node
    (nix-embedded-node? (list->nix-list-node `(1 2 3 4))))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_false],[])

m4_define([sexpr],
  [(make-nix-data-node
    (nix-list-node? (list->nix-list-node `(1 2 3 4))))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_true],[])

m4_define([sexpr],
  [(make-nix-data-node (nix-list-node? `(1 2 3 4)))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_false],[])

m4_define([sexpr],
  [(make-nix-data-node (nix-data-node? `(1 2 3 4)))])
check_unquoted([run_scheme 'sexpr'],[0],[nix_false],[])

m4_define([sexpr],
  [(make-nix-data-node
    (nix-data-node? (list->nix-list-node `(1 2 3 4))))])
